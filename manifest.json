{
    "manifest_version": 3,
    "name": "Nibble - flavortown ai detector",
    "description": "Detects AI-generated content on flavortown.",
    "version": "1.0",
    "icons": {
        "128": "128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://flavortown.hackclub.com/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [{
        "matches": ["https://flavortown.hackclub.com/*"],
        "resources": ["images/label.png"]
  }]
}